}

/* ================== 15. Multiplication Table ================== */
class Table {
    public static void main(String[] args) {
        int n = 5;
        for (int i = 1; i <= 10; i++)
            System.out.println(n + " x " + i + " = " + (n * i));
    }
}

/* ================== 16. Swap Without Temp ================== */
class Swap {
    public static void main(String[] args) {
        int a = 10, b = 20;
        a = a + b;
        b = a - b;
        a = a - b;
        System.out.println(a + " " + b);
    }
}

/* ================== 17. Count Vowels ================== */
class VowelCount {
    public static void main(String[] args) {
        String s = "java";
        int count = 0;
        for (char c : s.toCharArray())
            if ("aeiou".indexOf(c) != -1)
                count++;
        System.out.println(count);
    }
}

/* ================== 18. Reverse String ================== */
class ReverseString {
    public static void main(String[] args) {
        String s = "java", rev = "";
        for (int i = s.length() - 1; i >= 0; i--)
            rev += s.charAt(i);
        System.out.println(rev);
    }
}

/* ================== 19. Array Sum ================== */
class ArraySum {
    public static void main(String[] args) {
        int[] a = {1, 2, 3, 4};
        int sum = 0;
        for (int i : a)
            sum += i;
        System.out.println(sum);
    }
}

/* ================== 20. Largest in Array ================== */
class LargestArray {
    public static void main(String[] args) {
        int[] a = {10, 25, 5, 40};
        int max = a[0];
        for (int i : a)
            if (i > max) max = i;
        System.out.println(max);
    }
}